{
  "name": "@map-colonies/cookbook",
  "version": "1.0.0",
  "description": "This is map colonies cookbook generated from confluence",
  "main": "./dist/index.js",
  "scripts": {
    "build": "npm run copyassets:all",
    "start": "npm run copyassets:all && docsify serve docs --port 4000",
    "start:prod": "http-server docs --port 4000",
    "release": "standard-version",
    "prebuild": "npm run clean",
    "clean": "rimraf dist",
    "prepack": "npm run build",
    "spellcheck": "cspell lint \"docs/**/*.md\"",
    "copyassets:all": "npm run copyassets:cesium && npm run copyassets:turf",
    "copyassets:cesium": "copyfiles -u 3 \"./node_modules/cesium/Build/Cesium/**/*\" \"./docs/assets/examples/scripts\"",
    "copyassets:turf": "copyfiles -u 1 \"./node_modules/turf/turf.js\" \"./docs/assets/examples/scripts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MapColonies/cookbook.git"
  },
  "author": "MapColonies",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MapColonies/cookbook/issues"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "homepage": "https://github.com/MapColonies/cookbook#readme",
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "commitlint": "^11.0.0",
    "copyfiles": "^2.4.0",
    "cspell": "^5.20.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^4.3.5",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.0"
  },
  "dependencies": {
    "cesium": "^1.92.0",
    "docsify-cli": "^4.4.4",
    "http-server": "^14.1.0",
    "turf": "^3.0.14"
  }
}
